@model IEnumerable<EasyTourney.Models.tblUserEvents>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.tblUser.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tblUser.LastName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.tblUser.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tblUser.LastName)
        </td>
       
    </tr>
}

</table>
